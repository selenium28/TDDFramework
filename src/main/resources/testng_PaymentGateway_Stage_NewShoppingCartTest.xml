<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Payment Gateway Stage New Shopping Cart Test Suite" thread-count="1" parallel="tests" preserve-order="true">

	<parameter name="environment" 	  value="stagingdev-5" />
	<!-- 	Iteration value (minimum = 1, maximum = any) -->
	<parameter name="iteration"       value="3" />
	
	
	<listeners>
		<listener class-name="com.reports.extentreportslistener.ExtentReportListener" />
	</listeners>
    
    <test name="Payment Gateway Stage New Shopping Cart Test Cases" >
		<classes>
		
			<!-- Netregistry Virtualization -->
			<class name="com.paymentgateway.stage.netregistry.testcases.RegressionNewShoppingCartTest">
				<methods>
	            		<include name="testDomainOrderInNewShoppingCartForNewBTCustomerUsingNewCard" />
<!-- 	            		<include name="testDomainOrderInNewShoppingCartForReturningBTCustomerUsingNewCard" /> -->
	            </methods>
	        </class>
 
		</classes>
	</test>  
	
</suite> 